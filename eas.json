{
  "cli": {
    "version": ">= 7.3.0"
  },
  "build": {
    "preview":{
      "android":{
        "buildType":"apk"
      },
      "distribution":"internal",
      "channel":"preview",
      "env":{
        "SENTRY_AUTH_TOKEN": process.env.SENTRY_AUTH_TOKEN,
        "EXPO_PUBLIC_SENTRY_DSN":process.env.EXPO_PUBLIC_SENTRY_DSN
      }    
    },
    "devolpment":{
      "extends":"preview",
      "android":{
        "withoutCredentials": true,
        "developmentClient":true
      },
      "channel":"devolpment"
      },
      "production":{
        "channel":"production",
        "env":{
          "SENTRY_AUTH_TOKEN": process.env.SENTRY_AUTH_TOKEN,
        "EXPO_PUBLIC_SENTRY_DSN":process.env.EXPO_PUBLIC_SENTRY_DSN
        },
        "android":{
          "buildType":"apk"
        }    
      }
    }
  }

